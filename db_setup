CREATE TABLE if not exists cmd_collection(id INTEGER PRIMARY KEY AUTOINCREMENT, cmd TEXT, type INTEGER ,active INTEGER, amount INTEGER)
CREATE UNIQUE INDEX if not exists cmd_idx on cmd_collection(cmd)
CREATE TABLE if not exists replies(cmd_id INTEGER, id INTEGER, active INTEGER, has_arg INTERGER, tag VARCHAR(100), type INTERGER, hash VARCHAR(30), file_type VARCHAR(10), reply TEXT, stamp DATE, time_used INTEGER)
CREATE UNIQUE INDEX if not exists reply_idx0 on replies(cmd_id, id, active, has_arg)
CREATE        INDEX if not exists reply_idx1 on replies(cmd_id, tag, active)
CREATE TABLE if not exists cmd_alias(id INTEGER PRIMARY KEY AUTOINCREMENT, cmd TEXT, p_cmd_id INTEGER, active INTERGER)
CREATE UNIQUE INDEX if not exists alias_idx0 on cmd_alias(cmd)
CREATE INDEX if not exists alias_idx1 on cmd_alias(p_cmd_id)
